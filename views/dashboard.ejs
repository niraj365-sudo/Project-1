        align-items: center;
        justify-content: center;
        width: 300px;
        height: 350px;
        /* border: 2px solid black; */
        position: relative;
      }
      .upload-form{
        position: absolute;
        display: flex;
        justify-content: center;
        align-items: center;
        flex-direction: column;
        gap: 16px;
      }
      .form-input{
        height:33px;
        width: 250px;
        padding: 9px;
        box-shadow: 0 0 0 0;
      }
      .footer{
        width: 100%;
        height:350px;
        background-color: black;

      }
