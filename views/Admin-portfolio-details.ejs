<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Edit Detail</h5>
        <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form action="/image/admin/update/<%= image.id%>" method="post" enctype="multipart/form-data">
          <div class="form-group"><input type="number" name="price" value=" <%=image.address%> " placeholder="Enter Price" class="form-control"></div> <br>
          <div class="form-group"> <input type="text" name="address" value="  <%=image.address%>" placeholder="Enter your address" class="form-control"></div> <br>
          <div class="form-group"> <input type="number" name="contact" value= "<%=image.contact%>" placeholder="Enter your contact" class="form-control"></div> <br>
          <div class="form-group"> <input type="text" name="email" value=" <%=image.email%>" placeholder="Enter your email" class="form-control"></div> <br>         
          <textarea name="description" id="description" cols="60" rows="10" class="form-control" value=" <%=image.description%>" placeholder="Description"></textarea> <br>
          <input type="file" name="myImage" id="file" value=" <%=image.imageURL%> placeholder="Choose your image" style="margin-left: -10px;" class="form-control" >

        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
          <button type="submit" class="btn btn-primary"  >Upload</button>
        </div>
      </form>
    </div>
  </div>
